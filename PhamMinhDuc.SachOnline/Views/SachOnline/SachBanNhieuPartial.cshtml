@model IEnumerable<PhamMinhDuc.SachOnline.Models.SACH>
<hr />
<div class="row">
    <div class="col-lg-6 col-md-6">
        <div class="media-object-default">
            @foreach (var sach in Model.Take(3))
            {
                <div class="media">
                    <div class="media-left">
                        <a href="@Url.Action("BookDetail", "SachOnline", new { id = sach.MaSach })">
                            <img class="media-object imgsachbannhieu"
                                 src="~/Images/@sach.AnhBia"
                                 alt="@sach.TenSach"
                                 style="width:100px; height:140px;" />
                        </a>
                    </div>
                    <div class="media-body">
                        <h4 class="media-heading">
                            <a href="@Url.Action("BookDetail", "SachOnline", new { id = sach.MaSach })">
                                @sach.TenSach
                            </a>
                        </h4>
                        <p>@sach.MoTa</p>
                    </div>
                </div>
            }
        </div>
    </div>
    <div class="col-lg-6 col-md-6">
        <div class="media-object-default">
            @foreach (var sach in Model.Skip(3).Take(3))
            {
                <div class="media">
                    <div class="media-left">
                        <a href="@Url.Action("BookDetail", "SachOnline", new { id = sach.MaSach })">
                            <img class="media-object imgsachbannhieu"
                                 src="~/Images/@sach.AnhBia"
                                 alt="@sach.TenSach"
                                 style="width:100px; height:140px;" />
                        </a>
                    </div>
                    <div class="media-body">
                        <h4 class="media-heading">
                            <a href="@Url.Action("BookDetail", "SachOnline", new { id = sach.MaSach })">
                                @sach.TenSach
                            </a>
                        </h4>
                        <p>@sach.MoTa</p>
                    </div>
                </div>
            }
        </div>
    </div>
</div>
